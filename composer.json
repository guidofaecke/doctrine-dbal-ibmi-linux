{
    "name" : "guidofaecke/doctrine-dbal-ibmi-linux",
    "description" : "Doctrine DBAL module for DB2 on the IBM i platform, while using odbc drivers on linux",
    "type" : "library",
    "license" : "MIT",
    "authors" : [{
            "name" : "James Titcumb",
            "email" : "james@asgrim.com"
        }, {
            "name" : "Cassiano Vailati",
            "email" : "c.vailati@esconsulting.it"
        }, {
            "name" : "Guido Faecke",
            "email" : "guido@emcent.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require" : {
        "php" : "^7.3 || ^8.0",
        "ext-pdo" : "*"
    },
    "require-dev" : {
        "doctrine/coding-standard": "^9.0",
        "doctrine/dbal": "^2.13.5",
        "phpunit/phpunit": "^9.5.10",
        "slevomat/coding-standard": "^7.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload" : {
        "psr-4" : {
            "DoctrineDbalIbmiLinux\\" : "src/"
        }
    }
}
